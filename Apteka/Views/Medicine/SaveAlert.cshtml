@model IEnumerable<Apteka.Models.Operacja>
<div class="modal fade" tabindex="-1" role="dialog" id="grade_details_modal">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header" style="background-color:aquamarine">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h3 style="text-align:center" class="modal-title">Szczegóły oceny</h3>
            </div>
            <div class="modal-body">

                <h4>Pomyślnie zapisano transakcje:</h4>
                <hr />

                <table class="table">
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.Data)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Przychod)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Rozchod)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Netto)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Brutto)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.AspNetUsers.Email)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Lek.Nazwa)
                        </th>
                        <th></th>
                    </tr>

                    @foreach (var item in Model)
{
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Data)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Przychod)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Rozchod)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Netto)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Brutto)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AspNetUsers.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Lek.Nazwa)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Id_operacja }) |
                    @Html.ActionLink("Details", "Details", new { id = item.Id_operacja }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Id_operacja })
                </td>
            </tr>
}

                </table>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Zamknij</button>

            </div>

        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->
